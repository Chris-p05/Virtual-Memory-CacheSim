import CacheValuesCalculator
from FileTracer import FileTracer


class PageTableUsage:
    def __init__(self):
        self.cache_values_calculator = CacheValuesCalculator()
        self.file_tracer = FileTracer()
        self.total_page_table_entries = 0
        self.used_page_table_entries = 0
        self.page_table_wasted_bytes = 0
